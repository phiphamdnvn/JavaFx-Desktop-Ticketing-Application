<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="454.0" prefWidth="710.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <TitledPane alignment="CENTER" prefHeight="491.0" prefWidth="730.0" text="Delete Ticket" VBox.vgrow="ALWAYS">
         <content>
          <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="481.0" prefWidth="728.0">
            <children>
              <Label alignment="CENTER" layoutX="119.0" layoutY="234.0" style="&#10;" text="Choose Ticket ID" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="14.0" />
                     </font>
              </Label>
                  <ComboBox fx:id="chooseTicket" layoutX="256.0" layoutY="231.0" prefHeight="26.0" prefWidth="195.0" />
                  <Button layoutX="146.0" layoutY="353.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
                  <Button layoutX="508.0" layoutY="353.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
                  <ComboBox fx:id="chooseMovie" layoutX="256.0" layoutY="115.0" onAction="#chooseMovie" prefHeight="26.0" prefWidth="195.0" />
                  <Label alignment="CENTER" layoutX="127.0" layoutY="118.0" style="&#10;" text="Choose Movie" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
            </children>
          </AnchorPane>
         </content>
      </TitledPane>
  </children>
</VBox>
