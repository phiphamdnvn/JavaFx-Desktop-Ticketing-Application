                        <Image url="@../image/exit.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="107.0" layoutY="255.0" text="Movie Maintenance" />
                  <Label layoutX="321.0" layoutY="255.0" text="Ticket Maintenance" />
                  <Label layoutX="543.0" layoutY="255.0" text="Seat Maintenance" />
