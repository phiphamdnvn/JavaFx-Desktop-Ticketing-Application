                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="90.0" layoutY="83.0" onMouseClicked="#movieMaintenance" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/movie.jpg" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="516.0" layoutY="83.0" onMouseClicked="#seatMaintenance" pickOnBounds="true" preserveRatio="true">
