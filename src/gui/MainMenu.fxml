                     <image>
                        <Image url="@../image/ticket.jpg" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="298.0" layoutY="285.0" onMouseClicked="#exit" pickOnBounds="true" preserveRatio="true">
