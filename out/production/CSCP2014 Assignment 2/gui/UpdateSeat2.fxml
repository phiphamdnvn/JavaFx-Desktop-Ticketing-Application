<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
      <TitledPane alignment="CENTER" text="Update Vip Seat" VBox.vgrow="ALWAYS">
         <content>
          <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
            <children>
              <Label alignment="CENTER" layoutX="255.0" layoutY="72.0" style="&#10;" text="Seat Information" textAlignment="CENTER" wrapText="false">
                <font>
                  <Font size="18.0" />
                </font>
              </Label>
                  <Label layoutX="161.0" layoutY="139.0" text="Vip Seat" />
                  <Label layoutX="143.0" layoutY="215.0" text="Set New Status" />
                  <Button layoutX="128.0" layoutY="331.0" mnemonicParsing="false" onAction="#update" text="Update" />
                  <Button layoutX="463.0" layoutY="331.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
                  <TextField fx:id="vipSeat" editable="false" layoutX="245.0" layoutY="135.0" />
                  <RadioButton fx:id="yes" layoutX="255.0" layoutY="214.0" mnemonicParsing="false" text="Yes" />
                  <RadioButton fx:id="no" layoutX="355.0" layoutY="214.0" mnemonicParsing="false" text="No" />
                  <Label layoutX="423.0" layoutY="215.0" text="(*By default is no)" />
            </children>
          </AnchorPane>
         </content>
         <font>
            <Font size="14.0" />
         </font>
      </TitledPane>
  </children>
</VBox>
